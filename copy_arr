class HelloWorld {
    public static void main(String[] args) {
        System.out.println("Hello, World!");
        int arr[]=new int[]{12,45,67};
        ques1(arr);
    }
    public static void ques1(int arr[]){
        int arr1[]=new int[arr.length];
        for(int i=0;i<arr.length;i++){
            arr1[i]=arr[i];
        }
        for(int num:arr1){
            System.out.println(num);
        }
        
    }
}
